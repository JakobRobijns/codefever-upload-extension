{
  "manifest_version": 2,
  "name": "CodeFever upload extension",
  "version": "0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://uploads.slaask.com/visitors/screenshots/*"],
      "css": ["style.css"],
    "run_at": "document_start"
    }
  ]
}
